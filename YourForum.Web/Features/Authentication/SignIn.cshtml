@model YourForum.Web.Features.Authentication.SignIn.Command

<div class="page-header">
    <h1>Logga in</h1>
</div>

<div class="row">
    <div class="col-md-4">
        <form asp-action="signin" method="post">
            @Html.ValidationDiv()

            @Html.FormBlock(m => m.Email)

            @Html.FormBlock(m => m.Password)

            <div class="form-group">
                <button type="submit" class="btn btn-success pull-right">
                    Logga in
                </button>
            </div>
        </form>
    </div>
</div>